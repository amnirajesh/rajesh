package raj.mani.info;
import java.util.Scanner;
public class ReverseNumbers {
	public static void main(String[] args) {
	System.out.println("Enter the values to be reversed");
		Scanner a =new Scanner(System.in);
		int x = a.nextInt();
		int i,j=0;
		int s=x;
		while(s>0) {
			i=x%10;
			j=j*10+i;
			s=x/10;
		}
		System.out.println("Reversing of "+x+ "Numbers"+j); 	
	}
}
