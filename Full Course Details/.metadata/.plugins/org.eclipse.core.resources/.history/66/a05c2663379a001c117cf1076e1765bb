package raj.mani.info;

public class FabinacySeriase {
public static void main(String[] args) {
	double a=0,b=1;
	System.out.println(a);
	System.out.println(a+1);
	for (double  j = 2; j <= 50; j++) {		
		double c=a+b;
		a=b;
	b=c;
	System.out.println(c);
	}                                                 
}}                                                        
                                                                