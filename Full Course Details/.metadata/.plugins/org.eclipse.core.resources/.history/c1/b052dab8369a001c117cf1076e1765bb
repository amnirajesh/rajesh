package raj.mani.info;

public class FabinacySeriase {
public static void main(String[] args) {
	int a=0,b=1;
	System.out.println(a);
	System.out.println(a+1);
	for (int j = 2; j <= 10; j++) {		
		int c=a+b;
		a=b;
	b=c;
	System.out.println(c);
	}                                                 
}}                                                        
                                                                